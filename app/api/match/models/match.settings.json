{
  "kind": "collectionType",
  "collectionName": "matches",
  "info": {
    "name": "Match",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "date": {
      "type": "datetime",
      "required": true
    },
    "local": {
